{
  "name": "manage-system",
  "version": "0.0.0",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"electron .\"",
    "build": "vite build",
    "electron:serve": "concurrently -k \"yarn dev\" \"yarn electron\"",
    "electron:build": "vite build && electron-builder --config electron.config.json"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@howdyjs/mouse-menu": "^2.0.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "axios": "^1.3.5",
    "custom-electron-titlebar": "^4.2.7",
    "electron-custom-titlebar": "^4.0.2",
    "element-plus": "^2.2.36",
    "jsencrypt": "^3.3.2",
    "vite": "^4.4.9",
    "vue": "^3.0.4",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "build": {
    "appId": "com.wss.app",
    "directories": {
      "output": "builder"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "files": [
      "build/**/*",
      "main.js"
    ]
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "@vueuse/electron": "^10.4.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.1",
    "electron-builder": "^24.6.3",
    "jsencrypt": "^3.3.2",
    "node-sass": "^8.0.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "vite-plugin-electron": "^0.14.0-beta.0",
    "vue-tsc": "^1.0.24"
  }
}
